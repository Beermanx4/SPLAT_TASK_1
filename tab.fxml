<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.BorderPane?>
<BorderPane fx:id="borderPane" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.TabController">
   <bottom>
      <ButtonBar prefHeight="32.0" prefWidth="248.0" BorderPane.alignment="CENTER">
        <buttons>
            <Button fx:id="prev_btn" mnemonicParsing="false" onAction="#goToPrevPosition" text="Prev" />
          <Button fx:id="nxt_btn" mnemonicParsing="false" onAction="#goToNextPosition" text="Next" />
        </buttons>
      </ButtonBar>
   </bottom>
   <center>
      <ListView fx:id="text_data" prefWidth="248.0" BorderPane.alignment="CENTER" />
   </center>
</BorderPane>
